{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public/HandshakeApp/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "unsafe-none"
          }
        ]
      }
    ]
  }
}
